<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="12dp"
        android:top="12dp"
        android:right="12dp"
        android:bottom="12dp">
        <rotate android:fromDegrees="45">
            <shape>
                <size
                    android:height="10dp"
                    android:width="10dp"/>
                <solid android:color="@color/indicator_center"/>
                <corners android:radius="2dp"/>
                <stroke
                    android:width="4dp"
                    android:color="@color/indicator_stroke"/>
            </shape>
        </rotate>
    </item>
    <item
        android:id="@+id/indicatorColor"
        android:left="12dp"
        android:top="12dp"
        android:right="12dp"
        android:bottom="12dp">
        <rotate android:fromDegrees="45">
            <shape>
                <size
                    android:height="10dp"
                    android:width="10dp"/>
                <solid android:color="@color/indicator_center"/>
                <corners android:radius="2dp"/>
                <stroke
                    android:width="4dp"
                    android:color="@color/indicator_stroke_transparent"/>
            </shape>
        </rotate>
    </item>
</layer-list>
