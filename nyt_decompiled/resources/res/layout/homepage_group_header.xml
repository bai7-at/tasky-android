<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">
    <TextView
        android:id="@+id/homepage_group_header_status"
        android:visibility="gone"
        android:layout_alignParentStart="true"
        style="@style/GroupHeaderStatus"/>
    <ImageView
        android:id="@+id/homepage_group_header_title_icon"
        android:visibility="gone"
        android:layout_toEndOf="@+id/homepage_group_header_status"
        style="@style/GroupHeaderIcon"/>
    <TextView
        android:id="@+id/homepage_group_header_title"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/homepage_group_header_title_icon"
        style="@style/WidthWrapHeightWrap"/>
</merge>
